#<?cfg paf dictionary ?>
#
# Dictionary for IsrDarkStage policies
#
target: lsst.ip.pipeline.IsrDarkStage

definitions: {
    #input clipboard keys
    inputKeys: {
        type: "policy"
        dictionary: {
            definitions: {
                exposure: {
                    type: "string"
                    description: "specify the clipboard key of the exposure to process."
                    default: "Exposure"
                    maxOccurs: 1
                }        
                darkexposure: {
                    type: "string"
                    description: "Specify the clipboard key of the dark exposure to use"
                    default: "DarkExposure"
                    maxOccurs: 1
                }        

            }
        }
        maxOccurs: 1        
    }
    
    #output clipboard keys:
    outputKeys: {
        type: "policy"
        dictionary: {
            definitions: {
                darkSubtractedExposure: {
                    type: "string"
                    description: "specify output clipboard key of the corrected exposure"
                    maxOccurs: 1
                    default: "darkSubtractedExposure"
                }
            }
        }
        maxOccurs: 1
    }
    parameters: {
        type: "policy"
        dictionary: {
            definitions: {
                darkscaleKeyword: {
                    type: "string"
                    description: "Dark exposure time keyword"
                    default: "EXPTIME"
                    minOccurs: 1 
                }
                exposurescaleKeyword: {
                    type: "string"
                    description: "Exposure time keyword"
                    default: "EXPTIME"
                    minOccurs: 1 
                }
                
            }
        }
    }

}
