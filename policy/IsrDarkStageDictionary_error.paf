#<?cfg paf dictionary ?>
#
# Dictionary for IsrDarkStage policies
#
target: lsst.ip.pipeline.IsrDarkStage

definitions: {
    #input clipboard keys
    inputKeys: {
        type: "policy"
        dictionary: {
            definitions: {
                exposure: {
                    type: "string"
                    description: "specify the clipboard key of the exposure to process."
                    default: "Exposure"
                    maxOccurs: 1
                }        
                darkexposure: {
                    type: "string"
                    description: ""
                    default: "DarkExposure"
                    maxOccurs: 1
                }        
                darkscale: {
                    type: "string"
                    description: ""
                    default: "darkScale"
                    maxOccurs: 1
                }        
                exposurescale: {
                    type: "string"
                    description: ""
                    default: "exposureScale"
                    maxOccurs: 1
                }        

            }
        }
        maxOccurs: 1        
    }
    
    #output clipboard keys:
    outputKeys: {
        type: "policy"
        dictionary: {
            definitions: {
                darkCorrectedExposure: {
                    type: "string"
                    description: "specify output clipboard key of the corrected exposure"
                    maxOccurs: 1
                    default: "darkCorrectedExposure"
                }
            }
        }
        maxOccurs: 1
    }
}
